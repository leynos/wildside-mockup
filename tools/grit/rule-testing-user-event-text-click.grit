`$ue.click($target.$byText($label))` as $node where {
  $filename <: r".*(?:/__tests__/|\\__tests__\\|/tests/|\\tests\\|\\.test\\.|\\.spec\\.).*",
  $ue <: `userEvent`,
  $byText <: `getByText`,
  register_diagnostic(span=$node, message="[test/prefer-byrole-for-actions] Prefer ByRole when interacting (e.g. getByRole('button', { name: $label })).")
}

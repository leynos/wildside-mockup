`cy.$method('[data-testid=$id]')` as $node where {  $method <: `get`,
  register_diagnostic($method, message="[test/no-testid-selectors] Prefer a11y locators. Hint: reach for getByRole('button', { name: 'Save' }) or getByLabelText('Email') before falling back to data-testid. data-testid requires an ACCESSIBILITY justification.")
}

`document.$method($cls)` as $node where {
  $filename <: r".*(?:/__tests__/|\\__tests__\\|/tests/|\\tests\\|\\.test\\.|\\.spec\\.).*",
  $method <: `getElementsByClassName`,
  register_diagnostic($method, message="[test/no-raw-dom-lookup] Prefer accessible queries over raw DOM lookups in tests.")
}

{
  "name": "radix-daisyui-playground",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "ISC",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tokens:build": "node tokens/build/style-dictionary.js",
    "fmt": "bunx biome format --write src tests tools docs package.json biome.jsonc bunfig.toml",
    "lint": "bunx biome ci src tests tools docs",
    "check:types": "tsc --noEmit --skipLibCheck",
    "test": "bun test --preload ./tests/setup-happy-dom.ts",
    "test:e2e": "playwright test",
    "semantic:lint": "bunx biome ci src tests tools docs && bun run lint:classlist && uvx semgrep --config tools/semgrep-semantic.yml --include src/**/*.tsx --include tests/**/*.tsx && bunx stylelint 'src/**/*.css' --config tools/stylelint.config.cjs",
    "semantic": "bun run semantic:lint",
    "lint:classlist": "bun run scripts/check-classlist-length.ts"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-slider": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.0",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-router": "^1.133.32",
    "daisyui": "^5.0.0",
    "maplibre-gl": "^3.6.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@biomejs/biome": "^2.3.1",
    "@getgrit/cli": "^0.1.0-alpha.1743007075",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/postcss": "^4.1.16",
    "@tailwindcss/vite": "^4.1.16",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "bun-types": "^1.1.3",
    "happy-dom": "^15.11.2",
    "postcss": "^8.4.49",
    "semgrep": "^0.0.1",
    "style-dictionary": "^4.0.0",
    "stylelint": "^16.25.0",
    "stylelint-declaration-strict-value": "^1.10.11",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
